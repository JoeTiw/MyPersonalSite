      {{!--About section--}}
      {{#each about }}

    <section class="about_me py-5 background2" id="about">  

      <div class="container">
         <div class="row">
          <div class="col-md-12">

            <h1 class="text-center text-secondary">{{this.title}}</h1>
            <h4 class="text-center text-light py-3">{{this.subTitile}}</h4>

            <div class="abouttext intro-info py-4">

               <img src="{{this.image}}" alt="Profile Picture">

              
               <p class ="lead text-light">{{this.description}} </p>
            </div>
            </div>            

         </div>       
         
      <div class="row about-content py-5 text-light">

         <div class="blocktext col-six tab-full text-center">

            <h3>{{this.profile}}</h3>
            <div class="text-wrap" style="width: 25rem;">
            </div>

            <ul class="info-list">
               <li>
                  <strong>Fullname:</strong>
                  <span>Bhupin Tiwari</span>
               </li>
               <li>
                  <strong>Phone Number:</strong>
                  <span>920-917-9995</span>
               </li>
               <li>
                  <strong>Email:</strong>
                  <span>bhupintiwari@gmail.com</span>
               </li>

            </ul> <!-- /info-list -->

         </div>

         <div class="skillstext col-six tab-full">

            
            <h3 class="text-center py-3">{{this.skills}}</h3>
            <p class="lead text-light">{{this.skillDiscription}}</p>
            

